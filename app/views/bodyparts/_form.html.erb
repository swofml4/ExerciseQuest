<%= simple_form_for bodypart do |f| %>
  <div class="row">
    <div class="col-sm-6 form-group"><%= f.input :name, :autofocus => true %></div>
  </div>
  <div class="row">
    <div class="actions">
      <div class="col-xs-6"><%= f.submit :class => 'btn btn-default' %></div>
    </div>
<% end %>
  <div class="col-sm-2 col-xs-6">
    <%= button_to 'Back', bodyparts_path, :method => "get", :class => 'btn btn-default' %>
  </div>
  </div>